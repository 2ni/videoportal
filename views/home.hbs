<div id="movies-list">
  <div class="content">
    <ul class="movies">
      {{#each movies}}
        <li><a href="/play/{{this.url}}">
            <video> <source src="/movies/{{this.url}}" type="video/mp4"></video>
            <div class="progressbar"><div class="progress"></div></div>
            <div class="meta">
              <h2>{{fn2name this.name}}</h2>
              <div class="duration"></div>
            </div>
        </a></li>
      {{/each}}
    </ul>
    <ul class="dirs">
      {{#each dirs}}
      <li><a href="{{this.url}}">{{this.name}}</a></li>
      {{/each}}
    </ul>
  </div>
</div>
